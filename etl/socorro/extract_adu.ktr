    <sql>SELECT&#xa;  TO_DATE&#x28;date, &#x27;yyyy-MM-dd&#x27;&#x29;&#x3a;&#x3a;TIMESTAMP AS &#x22;date&#x22;&#xa;, product_guid&#xa;, product_name&#xa;, product_version&#xa;, CASE&#xa;	WHEN product_os_platform &#x3d; &#x27;Linux&#x27; THEN &#x27;Linux&#x27;&#xa;	WHEN product_os_platform &#x3d; &#x27;Darwin&#x27; THEN &#x27;Mac OS&#x2f;X&#x27;&#xa;	WHEN product_os_platform LIKE &#x27;Windows_&#x25;&#x27; THEN &#x27;Windows&#x27;&#xa;	ELSE &#x27;Other&#x27; END AS product_os_platform&#xa;, product_build_number&#xa;, CASE&#xa;	WHEN product_channel LIKE &#x27;esr&#x25;&#x27; THEN &#x27;esr&#x27;&#xa;	WHEN product_channel LIKE &#x27;nightly&#x25;&#x27; THEN &#x27;nightly&#x27;&#xa;	WHEN product_channel LIKE &#x27;aurora&#x25;&#x27; THEN &#x27;aurora&#x27;&#xa;	WHEN product_channel LIKE &#x27;beta&#x25;&#x27; THEN &#x27;beta&#x27;&#xa;	ELSE &#x27;release&#x27; END AS product_channel&#xa;, SUM&#x28;requests&#x29; AS adu_count&#xa;FROM blocklist_requests_by_day b&#xa;JOIN dates d ON d.date_id &#x3d; b.utc_date_id&#xa;JOIN products p ON p.product_id &#x3d; b.product_id&#xa;JOIN operating_systems o ON o.os_id &#x3d; b.os_id&#xa;JOIN product_channels c ON b.product_channel_id &#x3d; c.product_channel_id&#xa;WHERE DATE&#x28;date&#x29; &#x24;&#x7b;DATE&#x7d;&#xa;AND &#x28;&#x28;product_name IN &#x28;&#x27;Firefox&#x27;, &#x27;Fennec&#x27;, &#x27;Thunderbird&#x27;&#x29; AND product_version_major &#x3e;&#x3d; 3&#x29; OR &#x28;product_name &#x3d; &#x27;SeaMonkey&#x27; AND product_version_major &#x3e;&#x3d; 2&#x29; OR product_name &#x3d; &#x27;WebappRuntime&#x27; OR product_name &#x3d; &#x27;MetroBrowser&#x27;&#x29;&#xa;GROUP BY 1, 2, 3, 4, 5, 6, 7&#xa;ORDER BY 1, 2, 3, 4, 5, 6, 7&#xa;</sql>
    <limit>0</limit>
