    <script><![CDATA[^/blocklist/[123]              # blocklist schema version
/(?>%7B)?([^/]{1,36}?)(?>%7D)?
                               # ^^ product_guid
/(?=[^/]{3,30}/)               # product_version
(                                      #   version string
    (\d{1,2})                          #   major numeric
    \.(\d{1,2}+)                       #   minor numeric
    (?>
       ([^./][^/]{0,14}+)?   #   minor a/b/rc/pre
       (?=/)
      |
       \.(\d{1,2})                     #   sub_a numeric
       (?>
          ([^./][^/]{0,14}+)? #   sub_a a/b/rc/pre
          (?=/)
         |
          \.(\d{1,2})                  #   sub_b numeric
          ([^./][^/]{0,14}+)? #   sub_b a/b/rc/pre
          (?=/)
       )
    )
)
/
(?>                           # blocklist/2 attributes
  ([^\s/]{2,20})(?>[^/]*)/    # product_name
  ([^/]{8,14})[^/]*+/         # product_build_number
  ([^/]{1,50})/               # product_build_target

  (?>                         # locale raw_string
    (?>
      chrome://(?:.{0,108})\.properties
      # Note: some Linuxes are messed up and report chrome URL instead of locale
    )|(?>
      (                       # locale code
       (?>[a-z]{2,3})         # language
       (?>[-_][A-Za-z]{2}(?>[-_][\w]{2,8})?)? # region-dialect
      )
      (?>;[^/]+)?
    )|(?>
       [^/]*
    )
   )
/
  ([^/]{0,100})                # channel
/
   (?>
    (?>
      ([A-Za-z_0-9]{1,15})     # legitimate platform name
      %20
      (\d{1,2}(?>
       \.\d{1,2}(?>
       \.\d{1,2}(?>
       \.\d{1,2})?)?)?
      )
    )?
    (?>[^/]*)                  # extra platform cruft
   )
/
  ([^/]{0,100})/               # distribution_name
  ([^/]{0,100})/               # distribution_version
  (?>                          # new ping counters
    ([^/]{0,10})/              # num pings counter
    (?>
      ([^/]{0,10})/            # num total pings counter
      (?>
        ([^/]{0,10})/          # days since last ping counter
      )?
    )?
  )?
)?                             # end of optional blocklist/2 attributes
$]]></script>    <matcher>url</matcher>
