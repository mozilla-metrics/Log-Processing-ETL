    <name>Capture application_version</name>
    <type>RegexEval</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <script><![CDATA[^
(                                      #   version string
    (\d{1,2})                          #   major numeric
    \.(\d{1,2}+)                       #   minor numeric
    (?>
       ([^.].{0,14}+)?   #   minor a/b/rc/pre
       $
      |
       \.(\d{1,2})                     #   sub_a numeric
       (?>
          ([^.].{0,14}+)? #   sub_a a/b/rc/pre
          $
         |
          \.(\d{1,2})                  #   sub_b numeric
          ([^.].{0,14}+)? #   sub_b a/b/rc/pre
          $
       )
    )
)
]]></script>    <matcher>product_version_raw</matcher>
