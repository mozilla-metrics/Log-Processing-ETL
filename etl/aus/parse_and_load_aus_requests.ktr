    <script><![CDATA[^([^\s]{7,15})\s               # client_ip
(?>[^\[]++(?>(?<!\s)\[)?)+
\[([^\]]{26})\]                # request time :HH:mm:ss -0800

\s"GET\s
/update/[123]                  # AUS schema version
/([^/\s]{3,20})                # product_name

/(                             # product_version
  (?=[^/\s]{3,15}/)            # ensure version string size
  (\d{1,2})                    #   version_component_major
  \.(\d{1,2}[^./\s]{0,8})      #   version_component_minor
  (?:\.(\d{1,2}[^./\s]{0,8}))? #   version_component_a
  (?:\.(\d{1,2}[^./\s]{0,8}))? #   version_component_b
)

/([^/\s]{8,14})                # product_build_number
/([^/\s]{1,50})                # product_build_target

/(                             # locale raw_string
  (?>
     chrome://(?:.{0,108})\.properties
       # Note: some Linuxes are messed up and report chrome URL instead of locale
  )|(?>
    (                          # locale code
     ([a-z]{2,3})              # language
     (?>-([A-Z]{2}))?          # region
     (?>-([\w]{2,8}))?         # dialect
    )
  )|(?>
     [^/\s]{0,128}
  )
)

/([^/\s]{1,100})               # channel

(?>/
  (
   [^%\s]{1,50}                # legitimate platform with %20 separator
  |
   [^/\s]{1,50}                # fallback to capturing entire os field in platform if something like %OS_VERSION%
  )
  (?>%20                       # going after the OS version part. If this fails, the fallback above will match
    ( [^/\s]{1,50} )
  )?                           # version is optional since there are many bad OS_VERSION calls
  (?>
    /([^/\s]{1,100})           # distribution_name
    /([^/\s]{1,100})           # distribution_version
  )?                           # distribution is only in AUS schema version 3
)?                             # OS is only in AUS schema version 2
/update\.xml[^\s]*             # filename and extra url params
\sHTTP/1\.[01]"\s              # end of the request method/uri/version

(\d{3})\s                      # HTTP status code
(\d+)\s                        # bytes returned
"[^"]+"\s                      # referrer field sometimes a redirect from https://aus2.mozilla.org

"                              # User agent parsing, always quoted.
"?                             # If people spoof the user agent, they sometimes put in extra quotes.
(                              # The UA string
  (?>[^r]++(?>(?!rv:)r)?)+     # Uninteresting bits
  (?>
     rv:                       # Beginning of the gecko engine version token
     (?=[^;)]{3,15}[;)])       # ensure version string size
     (                         # Whole gecko version
       (\d{1,2})                   # version_component_major
       \.(\d{1,2}[^.;)]{0,8})      # version_component_minor
       (?>\.(\d{1,2}[^.;)]{0,8}))? # version_component_a
       (?>\.(\d{1,2}[^.;)]{0,8}))? # version_component_b
     )
     .*                        # More uninteresting bits
  )
 |
  .*                           # More uninteresting bits
)                              # End of UA string]]></script>    <matcher>line</matcher>
