  <step>
    <name>Postfilter and Date Parse</name>
    <type>UserDefinedJavaClass</type>
    <description/>
    <distribute>Y</distribute>
    <copies>2</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
