    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>RowProcessor</class_name>

        <class_source><![CDATA[import java.io.*;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    if (getVariable("wildcard") != null && !"xxx".equals(getVariable("wildcard"))) {
        String cmd = "find " + getVariable("directory") + " -name 'access_" + getVariable("wildcard") + "*' | sort";
        try {
            logMinimal("Running command: "+cmd);
            Process p = new ProcessBuilder(new String[] {"bash", "-c", cmd}).start();
            BufferedReader in = new BufferedReader(new InputStreamReader(p.getInputStream()));
            p.waitFor();
            String filename = null;
            while ((filename = in.readLine()) != null) {
                logMinimal(filename);
                Object[] rowData = RowDataUtil.allocateRowData(data.outputRowMeta.size());
                get(Fields.Out, "filename").setValue(rowData, filename);
                putRow(data.outputRowMeta, rowData);
            }
            in.close();
        } catch (Exception e) {
            e.printStackTrace();
            setErrors(1);
            stopAll();
        }
    }

    setOutputDone();
    return false;
}
]]></class_source>
