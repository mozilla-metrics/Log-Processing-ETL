  <step>
    <name>Get File Names</name>
    <type>UserDefinedJavaClass</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>

    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.File;
import java.util.ArrayList;
	


	static public ArrayList listFilesForFolder(String folderName, String regexpFilter) {
		File folder = new File(folderName);
		ArrayList fileList = new ArrayList();
		File[] files = folder.listFiles();
		
		if (files != null) {
			if (files.length == 0) {
				System.out.println("No files for "+folderName);
			}
			for (int i = 0; i < files.length; i++) {
				File file = files[i];
				String fileName = file.getName();
				if (fileName.matches(regexpFilter) && file.isDirectory()==false) {
					fileList.add(file);
				}
			}
			if (fileList.size()==0) {
				System.out.println("No files matched "+folderName+"/" + regexpFilter);
			}
		} else {
			System.out.println("Not a folder: "+folderName);
		}
		return fileList;
	}
	
	public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
	{
		Object[] r = getRow();
		if (r == null) {
			setOutputDone();
			return false;
		}

	    if (first)
	    {
	        first = false;
	    }
	    
	    String folderName  = get(Fields.In, "directory").getString(r);
	    String filterRegex = get(Fields.In, "filename_pattern").getString(r);
	    
		ArrayList fileList = listFilesForFolder(folderName, filterRegex);
		for (int i = 0; i < fileList.size(); i++) {
			r = createOutputRow(r, data.outputRowMeta.size());
			String fileName = ((File) fileList.get(i)).getName();
			long fileSize = ((File) fileList.get(i)).length();
			get(Fields.Out, "short_filename").setValue(r, fileName);
			get(Fields.Out, "fileName").setValue(r, folderName+"/"+fileName);
			get(Fields.Out, "size").setValue(r, fileSize);
			putRow(data.outputRowMeta, r);
		}

		return true;
	}
	
]]></class_source>
